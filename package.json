{
  "name": "state-middleware-manager",
  "version": "0.0.0-development",
  "description": "State accumulating/reducing middleware manager",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "npm run clean && tsc && npm run build:common",
    "build:common": "tsc --project tsconfig.common.json > /dev/null; npm run build:covertcommonext",
    "build:covertcommonext": "renamer --force --find '/\\.(j|t)s/' --replace '.c$1s' dist/common/**/*",
    "clean": "rimraf dist",
    "prepare": "husky",
    "test": "vitest test"
  },
  "author": "MOL Ads <MOLAds@mailonline.co.uk>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/types": "^19.5.0",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.0",
    "@semantic-release/npm": "^12.0.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "renamer": "^5.0.2",
    "semantic-release": "^24.1.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/johngeorgewright/state-middleware-manager.git"
  },
  "lint-staged": {
    "*.{md,json,js,jsx,ts,tsx,yml,yaml}": [
      "yarn prettier --ignore-path .gitignore --write"
    ]
  },
  "dependencies": {
    "tslib": "^2.8.0"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
